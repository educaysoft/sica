    var common_event_options = {
      "slideshow_id": "125510680",
      "slideshow_created_at": "Dec 10, 2018",
      "slideshow_category": "health-medicine",
      "slideshow_language": "es",
      "slideshow_format": "presentation"
    };

    $(".scribd-ad").find("a").click(function() {
      amplitude.getInstance().logEvent("scribd_ad_click", $.extend({}, common_event_options, {
        "ad_type": "MX_2",
        "follow_url": "https://www.scribd.com/slideshare"
      }))
    });

    $(".scribd-ad-rec").click(function() {
      amplitude.getInstance().logEvent("scribd_ad_rec_click", $.extend({}, common_event_options, {
        "ad_type": "110x82_reccs_2",
        "follow_url": "https://www.scribd.com/subscribe-now"
      }))
    });

    var exitPromoLogged = false
    $(document).on("mouseleave", function() {
      var exitAdCookie = $.cookie("scribd_ad_exit_slideshow_page")

      var dismissAd = function () {
        amplitude.getInstance().logEvent("exit_promo_dismissed", common_event_options)
        $.cookie("scribd_ad_exit_slideshow_page", true, { path: "/", expires: 1 })
        $("#scribd-ad-exit-modal").addClass("hidden")
      }

      if (!exitAdCookie) {
        if (!exitPromoLogged) {
          amplitude.getInstance().logEvent("exit_promo_shown", common_event_options)
          exitPromoLogged = true;
        }

        $("#scribd-ad-exit-modal").removeClass("hidden")

        $(document).on("keyup", function(event) {
          if (event.keyCode === 27) {
            dismissAd()
          }
        })

        $("#scribd-ad-exit-backdrop").on("click", function() {
          dismissAd()
        })

        $("#scribd-ad-exit-modal .close").on("click", function() {
          dismissAd()
        })

        $("#scribd-ad-exit-modal .cta").on("click", function() {
          amplitude.getInstance().logEvent("exit_promo_cta_clicked", common_event_options)
          $.cookie("scribd_ad_exit_slideshow_page", true, { path: "/", expires: 365 })
          $("#scribd-ad-exit-modal").addClass("hidden")
        })
      }
    })

    // Favorites update
  </script>
  <script>
    $('#close-sidebar').click(function() {
      $('#nav-sidebar').removeClass('open')
    })

    $('#open-sidebar').click(function() {
      $('#nav-sidebar').addClass('open')
    })

    $('#nav-sidebar').click(function() {
      $('#nav-sidebar').removeClass('open')
    })

    $('#sidebar-content').click(function(e) {
      e.stopPropagation()
    })
  </script>
